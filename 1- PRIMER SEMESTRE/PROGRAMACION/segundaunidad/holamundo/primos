num = int((input("ingrese el numero para saber si es primo o no")))
x = 1
contador = 0
while x <= num:
    if num % x == 0:
        contador = contador +1
        x = x +1
    elif contador == 2:
        print("el numero ",num," es primo")
    else:
        print("el numero ",num, " no es primo")
        break